<?php

namespace ChatBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use ChatBundle\Entity;

/**
 * @ORM\Entity
 * @ORM\Table(name="room")
 */
class Room {

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="string", length=100)
     */
    protected $name;

}
